{
  "name": "@resolid/dev",
  "version": "0.2.0",
  "private": false,
  "description": "Development utils for Resolid applications",
  "keywords": [
    "netlify",
    "nodejs",
    "react-router",
    "resolid",
    "vercel",
    "vite"
  ],
  "homepage": "https://www.resolid.tech",
  "license": "MIT",
  "author": {
    "name": "Huijie Wei",
    "email": "hello@resolid.tech"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resolid/framework.git",
    "directory": "packages/dev"
  },
  "files": [
    "dist",
    "env.d.ts"
  ],
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./env": {
      "types": "./env.d.ts"
    },
    "./http.server": {
      "types": "./dist/http.d.mts",
      "import": "./dist/http.mjs"
    },
    "./router": {
      "types": "./dist/router.d.mts",
      "import": "./dist/router.mjs"
    },
    "./routes": {
      "types": "./dist/routes.d.mts",
      "import": "./dist/routes.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.mts",
      "import": "./dist/vite.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@resolid/utils": "^1.2.7",
    "@vercel/nft": "^1.3.2",
    "fast-glob": "^3.3.3",
    "hono": "^4.12.3",
    "minimatch": "^10.2.4",
    "rolldown": "latest"
  },
  "devDependencies": {
    "@react-router/dev": "^7.13.1",
    "tsdown": "^0.20.3"
  },
  "peerDependencies": {
    "@netlify/types": "^2.3.0",
    "@react-router/dev": "^7.13.1",
    "@react-router/node": "^7.13.1",
    "isbot": "^5.1.35",
    "react-router": "^7.13.1",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": "^22.13.0 || >=24"
  }
}
