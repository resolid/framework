{
  "name": "@resolid/cache",
  "version": "1.0.0",
  "private": false,
  "description": "Type-safe Async Cache for TypeScript",
  "keywords": [
    "cache",
    "cache manager",
    "resolid"
  ],
  "homepage": "https://www.resolid.tech",
  "license": "MIT",
  "author": {
    "name": "Huijie Wei",
    "email": "hello@resolid.tech"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/resolid/framework.git",
    "directory": "packages/cache"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./stores": {
      "types": "./dist/stores.d.mts",
      "import": "./dist/stores.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "destr": "^2.0.5",
    "quick-lru": "^7.3.0"
  },
  "devDependencies": {
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^22.13.0 || >=24"
  }
}
