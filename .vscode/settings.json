{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.useESLintClass": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "npm.exclude": ["**/build/**", "**/.netlify/**", "**/.vercel/**"],
  "files.watcherExclude": {
    "**/.netlify/**": true,
    "**/.vercel/**": true,
    "**/build/**": true,
    "**/node_modules/**": true
  },
  "search.exclude": {
    "**/.netlify/**": true,
    "**/.vercel/**": true,
    "**/build/**": true
  },
  "editor.formatOnSave": true,
  "debug.javascript.codelens.npmScripts": "never",
  "editor.defaultFormatter": "Jme797.prettier-vscode-extension"
}
